{
    "model" : "gpt-3.5-turbo",
    "max_completion_tokens": 16,
    "max_prompt_tokens": 14,
    "max_total_tokens": 30,
    "response_format": {"type": "text"},
    
    "default": {
        "actor": "write only *default prompt error* 10 times",
        "method": "POST",
        "purpose": "fine-tune",
        "temperature": 0.7,
        "messages": [
            {"role": "system", "content": "Hello, how can I assist you today?"},
            {"role": "user", "content": "I want you to write *default message error* 10 times"}
        ]
    },

    "roles": {
        
        "raw-to-meaning": {
            "temperature" : 0.5,
            "actor":"Convert the given text into a meaningful text."
        },

        "md-auto": {
            "actor":"auto actor",
            "purpose": "assistants"
            
        },

        "md-historian": {
            "temperature" : 0.1,
            "actor":"historian actor"
        },
        
        "md-developer": {
            "temperature" : 1.0,
            "actor":"developer actor."
        },

        "md-mathematician": {
            "temperature" : 0.3,
            "actor":"mathematician actor"
        }

    }

}